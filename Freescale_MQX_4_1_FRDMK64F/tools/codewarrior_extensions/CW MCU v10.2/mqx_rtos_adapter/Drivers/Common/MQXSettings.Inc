%if %get(hasUserMallocFun, Bool) = 'yes'
%;**%>20 User memory allocation function name %>60: %get(userMallocFunName,Text)
%else %- hasUserMallocFun
%endif %- hasUserMallocFun
%;**%>18 User function for memory deallocation %>60: %get(hasUserDeallocFun,Text)
%if %get(hasUserDeallocFun, Bool) = 'yes'
%;**%>20 User function name %>60: %get(userDeallocFunName,Text)
%else %- hasUserDeallocFun
%endif %- hasUserDeallocFun
%;**%>16 Critical section %>60: %get(Symbol,Text)
