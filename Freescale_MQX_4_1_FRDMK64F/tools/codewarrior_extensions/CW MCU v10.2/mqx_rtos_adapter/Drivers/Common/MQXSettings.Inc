%if %get(hasUserEnterCriticalFun, Bool) = 'yes'
%;**%>20 User function name %>60: %get(userEnterCriticalFunName,Text)
%else %- hasUserEnterCriticalFun
%endif %- hasUserEnterCriticalFun
%;**%>18 User function for exiting critical section %>60: %get(hasUserExitCriticalFun,Text)
%if %get(hasUserExitCriticalFun, Bool) = 'yes'
