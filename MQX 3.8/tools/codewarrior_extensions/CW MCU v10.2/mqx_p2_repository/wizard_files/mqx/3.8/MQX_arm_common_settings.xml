				<applySetting id="com.freescale.cdt.debug.cw.CW_SHADOWED_PREF.Embedded Initialization.memConfigPath" value="${unevaluated:MQX_ROOT_DIR}/lib/${BoardName}.cw10/bsp/dbg/${BoardName}.mem" type="string"/>
				
