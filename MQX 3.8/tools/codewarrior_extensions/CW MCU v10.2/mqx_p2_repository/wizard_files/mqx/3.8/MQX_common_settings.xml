<?xml version="1.0"?>
<wizardExtension><newProjectExtension><launchSettings><settingsGroup id="MQX_common_settings">

	<settingsGroupRef id="MQX_common_settings_elf_importer"/>

	<applySetting id="com.freescale.cdt.debug.cw.processor" value="${ChipDebugSelect}" type="string"/>
	<applySetting id="com.freescale.cdt.debug.cw.CW_SHADOWED_PREF.${dbg_group} Debugger.processor" value="${ChipDebugSelect}" type="string"/>
	<applySetting id="com.freescale.cdt.debug.cw.core.settings.DebuggerCommonData.Processor Attr Name" value="com.freescale.cdt.debug.cw.CW_SHADOWED_PREF.${dbg_group} Debugger.processor" type="string"/>

	 <applySetting id="com.freescale.cdt.debug.cw.osawareness.osContributorID" mode="replace" type="string" value="com.freescale.os.mqx"/>
	 <applySetting
                        id="com.freescale.cdt.debug.cw.osawareness.rtospluginname"
                        mode="replace"
                        type="string"
                        value="MQX RTOS COLDFIRE">
                  </applySetting>
                  <applySetting
                        id="com.freescale.cdt.debug.cw.osawareness.targetos"
                        mode="replace"
                        type="string"
                        value="MQX OS for ColdFire">
                  </applySetting>
      
      <!-- Without this the OS awareness does not work -->
      <applySetting id="com.freescale.panel.list" mode="replace" type="list">                     
                     <listValue value="Debugger Common Settings"> </listValue>
                    <listValue value="CF Exceptions"> </listValue>
                    <listValue value="CF Reset"> </listValue>
                    <listValue value="CF Interrupt"> </listValue>
                    <listValue value="Embedded Download"> </listValue>
                    <listValue value="Debugger PIC Settings Panel"> </listValue>
                    <listValue value="Remote Debug"> </listValue>
                    <listValue value="Other Executables"> </listValue>
                    <listValue value="Symbolics"> </listValue>
                    <listValue value="osawareness"> </listValue>
      </applySetting> 
                  
</settingsGroup></launchSettings></newProjectExtension></wizardExtension>
