	      <applySetting id="com.freescale.cdt.debug.cw.CW_SHADOWED_PREF.Embedded Initialization.smp" value="true" type="boolean"/>				
				<applySetting id="com.freescale.cdt.debug.cw.SMPCores" type="list" mode="append">
					<listValue value="0"/>
					<!-- <listValue value="1"/> -->
				</applySetting>				
				<applySetting id="com.freescale.cdt.debug.cw.CoreNameList" type="list" mode="append">
					<listValue value="${CoreIdDbg}#0"/>
					<!-- <listValue value="${CoreIdDbg}#1"/> -->
				</applySetting>				
				<applySetting id="com.freescale.cdt.debug.cw.CW_SHADOWED_PREF.Embedded Initialization.useMemoryConfigFile" value="true" type="boolean"/>
				<applySetting id="com.freescale.cdt.debug.cw.CW_SHADOWED_PREF.Embedded Initialization.memConfigPath" value="${MQX_ROOT_DIR}/lib/${BoardName}.cw10/bsp/dbg/${ChipId}_LS.mem" type="string"/>        
        <!-- End added for MQX -->		
				
				<applySetting id="com.freescale.cdt.debug.cw.CW_SHADOWED_PREF.EPPC Debugger.processor" value="${ProcessorIdDbg}" type="string"/>
				<applySetting
					id="com.freescale.cdt.debug.cw.core.settings.DebuggerCommonData.Processor Attr Name"
					value="com.freescale.cdt.debug.cw.CW_SHADOWED_PREF.EPPC Debugger.processor" type="string">
				</applySetting>
				
