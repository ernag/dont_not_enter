				</applySetting>

        <!-- Added or moved from other file for MQX -->
        <applySetting id="com.freescale.cdt.debug.cw.osawareness.osContributorID" mode="replace" type="string" value="com.freescale.os.mqx.cortexm"/>
	       <applySetting
                        id="com.freescale.cdt.debug.cw.osawareness.rtospluginname"
                        mode="replace"
                        type="string"
                        value="MQX RTOS PPC">
                  </applySetting>
                  <applySetting
                        id="com.freescale.cdt.debug.cw.osawareness.targetos"
                        mode="replace"
                        type="string"
                        value="MQX OS for PowerPC">
                  </applySetting>	
                  
        <applySetting id="com.freescale.cdt.debug.cw.CW_SHADOWED_PREF.Embedded Initialization.useInitFile" value="true" type="boolean"/>
				<applySetting id="com.freescale.cdt.debug.cw.CW_SHADOWED_PREF.Embedded Initialization.initPath" value="${MQX_ROOT_DIR}/lib/${BoardName}.cw10/bsp/dbg/${ChipId}_BOOKE.tcl" type="string"/>
	         
