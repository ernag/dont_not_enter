<wizardExtension>
  <uiExtension>
	<pages>
	  <page id="mcu.mqxprojectwizard1.page.devices">
		<uiElement id="mcu.mqxprojectwizard1.chip.list">		 
		  <elementChoice id="mcu.mqxprojectwizard1.board.list.cf">  
		    <elementChoice name="M52223EVB Board" id="mcu.mqxprojectwizard1.board.ID_BOARD_M52223EVB"
        systemTypeId="com.freescale.cw.system.coldfire.MCF5222X.MCF52223"
